{
    "name": "mlledesmeules/expect",
    "description": "BDD assertion library for PHPUnit and Codeception",
	"keywords" : [
		"bdd", "assertions", "phpunit", "codeception", "unit testing"
	],
	"minimum-stability": "stable",
	"license": "MIT",
    "authors": [
        {
            "name": "Roxanne Desmeules",
            "email": "roxanne.desmeules@gmail.com"
        }
    ],
	"require" : {
		"php" : ">=5.5.0"
	},
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
	"replace": {
		"codeception/verify": "0.*"
	},
	"suggest": {
		"codeception/specify": "Highly readable test code blocks for PHPUnit and Codeception"
	},
	"autoload": {
		"files": ["src/function.php"]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.0"
		}
	}
}
